{
  "name": "rgbpiano",
  "version": "1.0.0",
  "description": "Piano RGB LED controller. Will light up LED strip when keys are pressed.",
  "main": "./out/main/index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "node ./build/no-ui/index-no-ui.js",
    "preview": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build && tsc -p tsconfig.no-ui.json",
    "electron-postinstall": "electron-builder install-app-deps && electron-rebuild",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "color": "^4.2.3",
    "midi": "^2.0.0",
    "node-ssdp": "^4.0.1",
    "rpi-ws281x-native": "^1.0.4",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@types/color": "^3.0.3",
    "@types/midi": "^2.0.0",
    "@types/node": "16.18.21",
    "@types/node-ssdp": "^4.0.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "electron": "^22.3.4",
    "electron-builder": "^24.4.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^1.0.21",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.7",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ozturkkl/RGBPiano.git"
  },
  "keywords": [
    "piano",
    "rgb",
    "led",
    "controller",
    "addressible"
  ],
  "author": "Kemal Ozturk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ozturkkl/RGBPiano/issues"
  },
  "homepage": "https://github.com/ozturkkl/RGBPiano#readme"
}
